{
  "name": "frontend-events-site",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "build": "ng build -prod --stats-json --environment=dev",
    "deploy": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --project dev",
    "start:prod": "ng serve -prod --environment=prod",
    "build:prod": "ng build -prod --stats-json --environment=prod",
    "deploy:prod": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --project live",
    "test": "ng test",
    "lint": "ng lint",
    "test:ci": "ng test --code-coverage --single-run",
    "start:poc": "ng serve --app=poc",
    "build:poc": "ng build --app=poc -prod --stats-json --environment=dev",
    "deploy:poc": "firebase deploy --only hosting --token \"$FIREBASE_TOKEN\" --project poc",
    "e2e": "ng e2e",
    "serve": "node scripts/server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.1.0",
    "@angular/cdk": "5.0.0",
    "@angular/common": "5.1.0",
    "@angular/compiler": "5.1.0",
    "@angular/core": "5.1.0",
    "@angular/forms": "5.1.0",
    "@angular/http": "5.1.0",
    "@angular/material": "5.0.0",
    "@angular/platform-browser": "5.1.0",
    "@angular/platform-browser-dynamic": "5.1.0",
    "@angular/router": "5.1.0",
    "@angular/service-worker": "5.1.0",
    "core-js": "2.5.1",
    "foundation-sites": "6.4.4-rc1",
    "hammerjs": "2.0.8",
    "rxjs": "5.5.2",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.1",
    "@angular/compiler-cli": "5.1.0",
    "@angular/language-service": "5.1.0",
    "@types/jasmine": "2.8.2",
    "@types/node": "8.0.53",
    "codelyzer": "4.0.1",
    "express-history-api-fallback": "2.2.1",
    "firebase-tools": "3.16.0",
    "jasmine-core": "2.8.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.2.0",
    "ts-node": "3.3.0",
    "tslint": "5.8.0",
    "typescript": "2.4.2"
  }
